
import math
import os
import random
import re
import sys


def factorial(n):
   
    a = 1
    for i in range(2,n+1):
        a = a * i
    return a
             

if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    n = int(input().strip())

    result = factorial(n)

    fptr.write(str(result) + '\n')

    fptr.close()
